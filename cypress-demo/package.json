{
  "name": "cypress-demo",
  "version": "1.0.0",
  "description": "Cypress E2E tests with Page Object Model",
  "scripts": {
    "test": "cypress run",
    "test:headed": "cypress run --headed",
    "test:headless": "cypress run --headless",
    "test:chrome": "cypress run --browser chrome",
    "test:firefox": "cypress run --browser firefox",
    "test:edge": "cypress run --browser edge",
    "open": "cypress open",
    "docker:compose:up": "docker-compose up cypress",
    "docker:compose:chrome": "docker-compose up cypress-chrome",
    "docker:compose:firefox": "docker-compose up cypress-firefox",
    "docker:compose:edge": "docker-compose up cypress-edge",
    "docker:compose:down": "docker-compose down",
    "docker:interactive": "docker run -it --rm -v .:/app -w /app --entrypoint bash cypress/included:latest"
  },
  "keywords": [
    "cypress",
    "e2e",
    "testing"
  ],
  "devDependencies": {
    "cypress": "^15.6.0",
    "cypress-mochawesome-reporter": "^3.8.2"
  }
}
